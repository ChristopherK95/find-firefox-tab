{
  "manifest_version": 2,
  "name": "Tab finder",
  "version": "0.1",
  "description": "Tab finder lalalalalala",
  "permissions": [
    "<all_urls>",
    "tabs"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "tab_finder@whatsyouridea.com",
      "strict_min_version": "59.0"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F12"
      }
    }
  }
}
